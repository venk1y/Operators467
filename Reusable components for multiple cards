import React from "react";
import "./App.css";
// ProfileCard component
const ProfileCard = ({ name, greeting, imageSrc }) => {
  return (
    <div className="profile-card">
      <img src={imageSrc} alt={`${name}'s profile`} className="profile-image" />

      <h2>{name}</h2>
      <p>{greeting}</p>
    </div>
  );
};

const App = () => {
  const users = [
    {
      name: "Venky",
      greeting: "Hello, welcome to my profile!",
      imageSrc: "data:image/jpeg;base64,",
    },
    {
      name: "Reddy",
      greeting: "Hello, welcome to my profile!",
      imageSrc: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTGv7BRaFaWNn6tQzOrd7j1IHAS1mIsVKc8lg&s",
    },
    {
      name: "Ganesh",
      greeting: "Hello, welcome to my profile!",
      imageSrc: "data:image/webp;base64,",
    },
  ];

  return (
    <div className="profile-container">
      {users.map((user, index) => (
        <ProfileCard
          key={index}
          name={user.name}
          greeting={user.greeting}
          imageSrc={user.imageSrc}
        />
      ))}
    </div>
  );
};

export default App;
